<%@ page language="java" contentType="text/html; charset=UTF-8"    pageEncoding="UTF-8"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt"%><!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd"><html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8"><title>查看订单详细</title><script type="text/javascript"	src="${pageContext.request.contextPath}/js/jquery/jquery-1.9.1.min.js"></script><link href="${pageContext.request.contextPath}/css/ts.css" rel="stylesheet" type="text/css" /><style type="text/css">.sidebar-nav {	padding: 9px 0;}@media ( max-width : 980px) {	/* Enable use of floated navbar text */	.navbar-text.pull-right {		float: none;		padding-left: 5px;		padding-right: 5px;	}}</style><script type="text/javascript">$(document).ready(function() {		$("#confirm").click(function(){		var remark = $("#remark").val();		var dealStatus = $("#dealStatus").val();		var id = $("input[name='id']").val();		$.post("${pageContext.request.contextPath}/manager/updateOrder", {remark:remark,dealStatus:dealStatus,id:id}, function(				data) {			if(data.resCode == 1){				window.parent.$.closePanel();				window.parent.$("#sb").click();			}					});			});});</script></head><body><form name="updateOrder" id="updateOrderForm" method="post" action="">		<table class="table">			<tbody>				<tr>					<td class="" style="text-align: right" >商品名称：</td>					<td class="" style="text-align: left">${order.goodsName }</td>				</tr>				<tr>					<td class="" style="text-align: right" >数量：</td>					<td class="" style="text-align: left">${order.num }</td>				</tr>				<tr>					<td class="" style="text-align: right" >商品单价：</td>					<td class="" style="text-align: left">${order.price }</td>				</tr><tr>					<td class="" style="text-align: right" >总价：</td>					<td class="" style="text-align: left">${order.totalPrice }</td>				</tr><tr>					<td class="" style="text-align: right" >应付款：</td>					<td class="" style="text-align: left">${order.payment }</td>				</tr><tr>					<td class="" style="text-align: right" >支付时间：</td>					<td class="" style="text-align: left"><fmt:formatDate value="${order.payTime }" pattern="yyyy-MM-dd HH:mm:ss"/></td>				</tr><tr>					<td class="" style="text-align: right" >订单生成时间：</td>					<td class="" style="text-align: left"><fmt:formatDate value="${order.orderTime }" pattern="yyyy-MM-dd HH:mm:ss"/></td>				</tr><tr>					<td class="" style="text-align: right" >收件人姓名：</td>					<td class="" style="text-align: left">${order.attentionName }</td>				</tr><tr>					<td class="" style="text-align: right" >寄送地址：</td>					<td class="" style="text-align: left">${order.liveProvince }${order.liveCity }${order.liveArea }${order.attentionAddress }</td>				</tr><tr>					<td class="" style="text-align: right" >邮政编码：</td>					<td class="" style="text-align: left">${order.postCode }</td>				</tr><tr>					<td class="" style="text-align: right" >收件人电话：</td>					<td class="" style="text-align: left">${order.mobilePhone }</td>				</tr><tr>					<td class="" style="text-align: right" >交易方式：</td>					<td class="" style="text-align: left"><c:choose>									<c:when test="${order.isNegotiate eq 0 }">网上直接支付</c:when>									<c:when test="${order.isNegotiate eq 1 }">面谈</c:when>									<c:otherwise></c:otherwise>								</c:choose></td>				</tr>				<tr>					<td class="" style="text-align: right" >购买用户联系电话：</td>					<td class="" style="text-align: left">${order.mobileNumberBuy }</td>				</tr>				<tr>					<td class="" style="text-align: right" >交易状态：</td>										<td class="" style="text-align: left"><c:choose>									<c:when test="${order.tradeStatus eq 0 }">未形成订单</c:when>									<c:when test="${order.tradeStatus eq 1}">形成订单</c:when>									<c:when test="${order.tradeStatus eq 2}">交易完成</c:when>									<c:when test="${order.tradeStatus eq 3}">订单取消</c:when>									<c:when test="${order.tradeStatus eq 4}">支付中</c:when>							<c:when test="${order.tradeStatus eq 5}">支付失败</c:when>							<c:when test="${order.tradeStatus eq 6}">支付超时</c:when>									<c:otherwise></c:otherwise>								</c:choose></td>				</tr>								<tr>					<td class="" style="text-align: right" >订单状态：</td>					<td class="" style="text-align: left"><select name="dealStatus" id="dealStatus">										<option value="0" <c:if test="${order.dealStatus eq 0 }">selected = "selected"</c:if>>未处理</option>										<option value="1" <c:if test="${order.dealStatus eq 1 }">selected = "selected"</c:if>>已处理</option>									</select></td>				</tr>				<tr>					<td class="" style="text-align: right" >备注：</td>					<td class="" style="text-align: left">						<textarea rows="10" cols="60" name="remark" id="remark">${order.remark }</textarea>					</td>				</tr>				<tr>					<td style="text-align: center" colspan="2">						<div class="btn" style="margin-top: 20px;"><span id="confirm" >提交</span></div>						<input type="hidden" name="id"  value="${order.id }"/>					</td>					<!-- <td style="text-align: center"><input type="button" value="::返回::" onclick="window.history.back()" /></td> -->				</tr>			</tbody>		</table>	</form></body></html>