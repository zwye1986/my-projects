<%@ page language="java" pageEncoding="UTF-8"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt"%><c:set var="ctx" value="${pageContext.request.contextPath }"	scope="request"></c:set><!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd"><html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8"><link href="${ctx }/css/ts.css" rel="stylesheet" type="text/css" /><script type="text/javascript" src="${ctx }/js/jquery/jquery-1.6.4.js"></script><script type="text/javascript" src="${ctx }/js/jquery.maklon.tools.js"></script><link rel="stylesheet" href="${ctx }/css/jquery_maklon_tools.css" /><script type="text/javascript">$(document).ready(		function() {			$("#confirm").click(function(){				window.parent.$.closePanel();			});			$("#cancel").click(function(){				window.parent.$.closePanel();			});		});	</script></head><body><div class="ts_c">请至少选中一条记录</div><div class="btn"><span id="confirm" >确定</span><span id="cancel">取消</span></div></body></html>