<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="OrderIdRecord">
<insert id="insertOrderIdRecord" parameterType="OrderIdRecord">
insert into ec_orderid_record (orderid,amount,createtime,userid) values (#{orderId},#{amount},#{createTime},#{userId});
</insert>
</mapper>