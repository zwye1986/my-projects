<?xml version="1.0" encoding="UTF-8"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">	<changeSet id="addecUserSign" author="hepei">			<createTable tableName="ec_user_sign">			<column name="id" type="varchar(128)">				<constraints primaryKey="true" nullable="false" />			</column>			<column name="mobilePhone" type="varchar(20)">				<constraints nullable="true" />			</column>			<column name="signdatetime" type="datetime">				<constraints nullable="true" />			</column>			<column name="signbenefit" type="decimal(16,4)">				<constraints nullable="true" />			</column>			<column name="newSignCount" type="int(11)">				<constraints nullable="true" />			</column>			<column name="content" type="varchar(255)">				<constraints nullable="true" />			</column>					</createTable>		<createIndex tableName="ec_user_sign"			indexName="ec_user_sign_signdatetime">			<column name="signdatetime" type="datetime"></column>		</createIndex>				<createIndex tableName="ec_user_sign"			indexName="ec_user_sign_mobilePhone">			<column name="mobilePhone" type="varchar(20)"></column>		</createIndex>				<addColumn tableName="ec_user_game_relation">		<column name="clickdate" type="datetime">		<constraints nullable="true" />		</column>		</addColumn>	</changeSet>		<changeSet id="addSign_in_benefitConfig" author="hepei">		<sql>		    insert into 			ec_system_config VALUES			('105', 'Sign_in_benefit', '0.1', '每次签到获取的金额,单位为元');		</sql>	</changeSet>		<include file="/Users/zhangwenyun/Documents/efinance/src/test/resources/events_zhangwenyun_1.xml"/>	</databaseChangeLog>