<?xml version="1.0" encoding="UTF-8"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">	<changeSet id="modifyCoolposOrder" author="hepei">		 <addColumn tableName="ec_order">			<column name="cancel_time" type="datetime"></column>			<column name="serialNumber" type="varchar(128)"></column>		</addColumn> 			 <sql>		DROP EVENT IF EXISTS  UPDATE_PAYFORFAILURE_ORDER;	</sql>		<createProcedure >		<![CDATA[ 			CREATE EVENT UPDATE_PAYFORFAILURE_ORDER ON SCHEDULE EVERY 15 MINUTE  ON COMPLETION			NOT PRESERVE ENABLE DO begin			 UPDATE ec_order SET tradeStatus=6  WHERE payTime<DATE_ADD(NOW(),INTERVAL -12 HOUR) AND tradeStatus=4;			end;			]]>	</createProcedure>	</changeSet></databaseChangeLog>